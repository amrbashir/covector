{
  "name": "@covector/toml",
  "version": "0.0.0",
  "files": [
    "pkg/covector_toml_bg.wasm",
    "pkg/covector_toml.js",
    "pkg/covector_toml.d.ts",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "wasm-pack build --release --no-pack --target nodejs && rimraf pkg/.gitignore",
    "postbuild": "cp ./pkg/covector_toml_bg.wasm ../action/dist && cp ./pkg/covector_toml.js ../action/dist && npm run build:ts",
    "build:debug": "wasm-pack build --dev --no-pack --target nodejs && rimraf pkg/.gitignore && npm run postbuild",
    "build:ts": "tsc -b",
    "clean": "rimraf dist tsconfig.tsbuildinfo node_modules",
    "pretest": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.1",
    "rimraf": "^3.0.2"
  }
}
